    }
    bool loginUser () {
        string username, password, line;
        clearScreen();
        cout << "\t\t\t===================" << endl;
        cout << "\t\t\t       LOGIN       " << endl;
        cout << "\t\t\t===================" << endl;
        cout << "\t\t\tEnter username: ";
        cin >> username;
        cout << "\t\t\tEnter password: ";
        cin >> password;

        if (username == "admin" && password == "adminpass") {
            currentUser .username = username;
            currentUser .password = password;
            currentUser .fullName = "Administrator";
            currentUser .program = "Admin Program";
            currentUser .section = "Admin Section";
            progressFile = "admin_progress.txt";
            return true;
        } else {
            cout << "Invalid username or password!" << endl;
        }
        ifstream inFile("accounts.txt");
        if (!inFile.is_open()) {
            cout << "Error: Unable to open file!\n";
            return false;
        }
        while (getline(inFile, line)) {
            size_t pos = 0;
            string data[8];
            int i = 0;
            while ((pos = line.find(",")) != string::npos && i < 7) {
                data[i] = line.substr(0, pos);
                line.erase(0, pos + 1);
                i++;
            }
            data[i] = line;
            if (data[0] == username && data[1] == password) {
                currentUser .username = data[0];
                currentUser .password = data[1];
                currentUser .fullName = data[2];
                currentUser .program = data[3];
                currentUser .section = data[4];
                currentUser .gender = data[5];
                currentUser .birthday = data[6];
                currentUser .yearLevel = stoi(data[7]);
                inFile.close();
                progressFile = currentUser .username + "_progress.txt";
                loadProgress();
                return true;
            }
        }
        inFile.close();
        return false;
    }

    void loadProgress() {
        ifstream inFile(progressFile);
        quizScores.clear();
        if (inFile) {
            string subject;
            int score;
            while (inFile >> subject >> score) {
                quizScores[subject] = score;
            }
        }
        string subjects[] = {"Applied_Ethics", "Purposive_Communication", "National_Service_Training_Program_2"};
        for (string subject : subjects) {
            if (quizScores.find(subject) == quizScores.end()) {
                quizScores[subject] = -1;
            }
        }
        inFile.close();
    }

   void loadAnswersFromFile(const string& username) {
    ifstream inputFile(username + "_answers.txt");
    if (!inputFile) {
        cerr << "Error: Could not open file " << username + "_answers.txt" << endl;
        return;
    }

    string subject;
    int questionIndex;
    char answer;

    userAnswers.clear();

    while (inputFile >> subject >> questionIndex >> answer) {
        if (userAnswers[subject].size() <= questionIndex) {
            userAnswers[subject].resize(questionIndex + 1);
        }
        userAnswers[subject][questionIndex] = answer;
    }
    inputFile.close();
}

void saveAnswersToFile() {
    ofstream file(currentUser .username + "_answers.txt");
    if (file.is_open()) {
        for (const auto& subject : userAnswers) {
            for (size_t i = 0; i < subject.second.size(); ++i) {
                if (subject.second[i] != '\0') {
                    file << subject.first << " " << i << " " << subject.second[i] << "\n";
                }
            }
        }
        file.close();
    } else {
        cerr << "Error: Unable to open file for writing!" << endl;
    }
}
    void saveProgress() {
        ofstream outFile(progressFile);
        for (const auto& entry : quizScores) {
            outFile << entry.first << " " << entry.second << endl;
        }
        outFile.close();
    }

    void showProgress() {
    loadProgress();

    clearScreen();
    cout << "\t\t\033[30;101m||======================================================================||" << endl;
    cout << "\t\t||\033[107m                         QUIZ PROGRESS REPORT                         \033[101m||" << endl;
    cout << "\t\t||\033[107m----------------------------------------------------------------------\033[101m||" << endl;
    cout << "\t\t||\033[107m Fullname: " << setw(58) << left << currentUser .fullName << " \033[101m||" << endl;
    cout << "\t\t||\033[107m Program & Section: " << setw(49) << left << (currentUser .program + " - " + currentUser .section) << " \033[101m||" << endl;
    cout << "\t\t||\033[107m----------------------------------------------------------------------\033[101m||" << endl;
    cout << "\t\t||\033[107mSubject                       | Score  |    Percentage    | Status    \033[101m||" << endl;
    cout << "\t\t||\033[107m----------------------------------------------------------------------\033[101m||" << endl;

    int totalScore = 0, totalPossible = 0;
    string subjects[] = {"Ethics", "PURPOSIVE_COMMUNICATION", "NSTP_2", "Discrete_Mathematics", "PATHFIT_2", "FILDIS", "Science_Tech_and_Society", "Art_Appreciation", "Computer_Programming_2"};

    for (const string& subject : subjects) {
        string status;
        int score = quizScores.count(subject) ? quizScores[subject] : -1;
        double percentage = 50 + ((static_cast<double>(score) / 25) * 50);

        if (score == -1) {
            status = "NOT TAKEN";
        } else if (score == 0) {
            status = "FAILED";
        } else {
            status = (percentage >= 75) ? "PASSED" : "FAILED";
            totalScore += score;
            totalPossible += 25;
        }

        cout << "\t\t||\033[107m" << setw(30) << left << subject
             << "| " << setw(7) << left << (score >= 0 ? to_string(score) + "/25" : "N/A")
             << "| " << setw(17) << left << (score >= 0 ? to_string(percentage) + "%" : "N/A")
             << "| " << setw(9) << left << status << " \033[101m||" << endl;
        cout << "\t\t||\033[107m----------------------------------------------------------------------\033[101m||" << endl;
    }

    double overallPercentage = (totalPossible > 0) ? (50 + ((double)totalScore / totalPossible) * 50) : 50;
string overallStatus = (overallPercentage >= 75) ? "PASSED" : "FAILED";
    cout << "\t\t||\033[107mTotal Score: " << setw(2) << totalScore << "/" << totalPossible
         << " | Overall Percentage: " << setw(6) << overallPercentage << "%";
    if (totalPossible > 0) {
        cout << " | " << setw(18) << overallStatus << " \033[101m||" << endl;
    } else {
        cout << " | NOT TAKEN \033[101m||" << endl;
    }
    cout << "\t\t||======================================================================||\033[0m" << endl;

    char reviewChoice;
    cout << "\nDo you want to review any quiz results? (y/n): ";
    cin >> reviewChoice;

    if (reviewChoice == 'y' || reviewChoice == 'Y') {
        int quizChoice;
        cout << "\nChoose a subject to review (1. Ethics, 2. PURPOSIVE_COMMUNICATION, 3. ESP): ";
        cin >> quizChoice;

        if (quizChoice >= 1 && quizChoice <= 3) {
            reviewQuiz(subjects[quizChoice - 1]);
        } else {
            cout << "Invalid choice!\n";
        }
    }

    system("pause");
}

    void reviewQuiz(const string& subject) {

    map<string, vector<pair<string, char>>> quizQuestions = {
        {"Ethics", {
            {"1. Ethics is primarily concerned with: \na)  Legal rules. \ntb)  Moral principles. \nc)  Social customs.", 'b'},
                {"2. What is a conflict of interest? \na)  A disagreement with a colleague. \nb)  A situation where personal interest clashes with professional duty. \nc)  A misunderstanding at work.", 'c'},
                {"3. Beneficence means: \na)  Doing no harm. \nb)  Acting for the good of others. \nc)  Being fair.", 'b'},
                {"4. What is an ethical dilemma? \na)  A simple decision. \nb)  A situation with conflicting moral values. \nc)  A legal problem.", 'b'},
                {"5. Which is crucial for ethical decision-making? \na)  Ignoring emotions. \nb)  Considering all stakeholders. \nc)  Rushing to judgment.", 'b'},
                {"6. What is the impact of social media on society? \na)  Only positive. \nb)  Only negative. \nc)  Both positive and negative. ", 'c'},
                {"7. What is the role of PURPOSIVE COMMUNICATION communication in STS? \na)  Irrelevant. \nb)  Crucial for bridging the gap between PURPOSIVE COMMUNICATION and society. \nc)  Only for scientists.", 'b'},
                {"8. What is the role of policy in shaping technological development? \na)  No role. \nb)  A significant role in guiding and regulating technological advancements. \nc)  A minor role.", 'b'},
                {"9. What is a key characteristic of disruptive technologies?\na)  Incremental improvements.\nb)  Radical changes that overturn existing markets or industries.\nc)  Maintaining the status quo.", 'b'},
                {"10. What is technological obsolescence?\na)  Technology becoming more efficient.\nb)  Technology becoming outdated and replaced.\nc)  Technology becoming more user-friendly.", 'b'},
                {"11. What is the impact of globalization on technology? \na)  Increased interconnectedness and collaboration in technological development.\nb) No impact.\nc)  Decreased technological advancements.", 'a'},
                {"12. What is technological obsolescence?\na)  Technology becoming more efficient.\nb)  Technology becoming outdated and replaced.\nc)  Technology becoming more user-friendly.", 'b'},
                {"13. What is technological obsolescence?\na)  Technology becoming more efficient.\nb)  Technology becoming outdated and replaced.\nc)  Technology becoming more user-friendly.", 'b'}
            }},
        {"PURPOSIVE_COMMUNICATION", {
            {"1. The Scientific Revolution is associated with which period? \na)  Ancient Greece \nb)  The Middle Ages \nc)  The 16th and 17th centuries ?", 'c'},
                {"2. Which invention revolutionized communication in the 19th century? \na)  The printing press \nb)  The telephone ? \nc)  The internet", 'b'},
                {"3. The development of the automobile significantly impacted: \na)  Only transportation. \nb)  Urban planning and infrastructure. \nc)  All of the above", 'c'},
                {"4. What is a major ethical concern regarding genetic engineering? \na)  Increased food production \nb)  Potential for unforeseen consequences \nc)  Improved human health.", 'b'},
                {"5. The digital divide refers to: \na)  The gap between rich and poor countries \nb)  The unequal access to technology and information \nc) The difference in computer speeds.", 'b'},
                {"6. Nanotechnology deals with materials at what scale? \na)  Microscopic \nb)  Macroscopic \nc)  Nanoscopic", 'c'},
                {"7. Biotechnology is primarily concerned with: \na)  The study of rocks \nb)  The manipulation of biological systems \nc)  The study of stars.", 'b'},
                {"8. Which of these is a potential benefit of artificial intelligence (AI)? \na)  Job displacement \nb)  Improved efficiency and automation \nc)  Increased bias and discrimination", 'b'},
                {"9. What is a major environmental concern related to technology? \na)  Increased biodiversity \nb)  Pollution and waste generation \nc)  Improved air quality", 'b'},
                {"10. Sustainable technology aims to: \na)  Maximize economic growth at all costs \nb)  Minimize environmental impact \nc)  Balance economic and environmental needs", 'c'},
                {"11. The development of renewable energy sources is driven by: \na)  Decreasing fossil fuel prices \nb)  Concerns about climate change \nc)  Increased reliance on fossil fuels", 'b'},
                {"12. What is a key aspect of responsible innovation? \na)  Ignoring ethical considerations \nb)  Prioritizing economic gains \nc)  Engaging in public dialogue and ethical reflection", 'c'},
                {"13. What is technological determinism? \na)  Technology shapes society \nb)  Society shapes technology \nc)  Technology and society are independent", 'a'},
                {"14. What is social constructivism in relation to technology? \na)  Technology is solely determined by its physical properties \nb)  Society shapes the development and use of technology \nc)  Technology develops independently of social forces", 'b'},
                {"15. What is the impact of social media on society? \na)  Only positive \nb)  Only negative \nc)  Both positive and negative", 'c'},
                {"16. What is a major concern regarding the use of big data? \na)  Increased efficiency \nb)  Potential for privacy violations \nc)  Improved decision-making", 'b'},
                {"17. What is the role of PURPOSIVE COMMUNICATION communication in STS? \na)  Irrelevant \nb)  Crucial for bridging the gap between PURPOSIVE COMMUNICATION and society \nc)  Only for scientists", 'b'},
                {"18. What is technological literacy? \na)  The ability to use technology \nb)  Understanding the societal impacts of technology \nc)  Knowing how to code", 'b'},
                {"19. What is the relationship between PURPOSIVE COMMUNICATION and technology? \na)  Completely independent \nb)  PURPOSIVE COMMUNICATION informs technology and technology drives further scientific inquiry \nc)  Technology informs PURPOSIVE COMMUNICATION, but PURPOSIVE COMMUNICATION has no impact on technology", 'b'},
                {"20. What is the role of policy in shaping technological development? \na)  No role \nb)  A significant role in guiding and regulating technological advancements \nc)  A minor role", 'b'},
                {"21. What is the concept of technological convergence? \na)  Technologies becoming less interconnected \nb)  Different technologies merging and integrating \nc)  Technologies becoming obsolete", 'b'},
                {"22. What is a key characteristic of disruptive technologies? \na)  Incremental improvements \nb)  Radical changes that overturn existing markets or industries \nc)  Maintaining the status quo", 'b'},
                {"23. What is technological obsolescence? \na)  Technology becoming more efficient \nb)  Technology becoming outdated and replaced \nc)  Technology becoming more user-friendly", 'b'},
                {"24. What is the impact of globalization on technology? \na)  No impact \nb)  Increased interconnectedness and collaboration in technological development \nc)  Decreased technological advancements", 'b'},
                {"25. What is the role of ethics in technological development? \na)  Unimportant \nb)  Essential for responsible innovation and addressing potential societal harms \nc)  Only relevant for certain technologies", 'b'}
            }},
        {"ESP", {
            {"1. What is the core value of ESP? \na) Honesty \nb) Greed \nc) Anger", 'a'},
            {"2. Respect is important because? \na) It shows care \nb) It makes you rich \nc) It is optional", 'a'},
        }}
    };
    if (quizQuestions.find(subject) == quizQuestions.end()) {
        cout << "Invalid subject!\n";
        return;
    }

    clearScreen();
    cout << "\nReviewing " << subject << " Quiz...\n";

    for (size_t i = 0; i < quizQuestions[subject].size(); ++i) {
        const auto& q = quizQuestions[subject][i];
        cout << q.first << "\n";
        char correctAnswer = q.second;

        char userAnswer = (userAnswers.find(subject) != userAnswers.end() && userAnswers[subject].size() > i)
                          ? userAnswers[subject][i]
                          : '\0';

        if (userAnswer == '\0') {
            cout << "Your answer: Not attempted\n";
        } else {
            cout << "Your answer: " << userAnswer << "\n";
            cout << "Correct answer is: " << correctAnswer << "\n";
            if (userAnswer == correctAnswer) {
                cout << "Feedback: Correct!\n";
            } else {
                cout << "Feedback: Incorrect!\n";
            }
        }

        cout << "-----------------------------------\n";
    }

    cout << "End of review for " << subject << " Quiz.\n";
    system("pause");
}

