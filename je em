#include <iostream>
#include <fstream>
#include <string>
#include <map>
#include <windows.h>
#include <iomanip>
#include <vector>
#include <ctime>
using namespace std;

void loading();

struct User {
    string username, password, fullName, program, section, gender, birthday;
    int yearLevel;
};

void cS() {
    system("CLS");
}

class QuizSystem {
private:
    User currentUser ;
    map<string, int> quizScores;
    map<string, vector<char>> userAnswers;
    string progressFile;

public:
    User getCurrentUser () const {
        return currentUser ;
    }

    void printSeparator() {
        cout << "============================" << endl;
    }

    void clearScreen() {
        system("CLS");
    }

    void registerUser () {
        User newUser ;
        clearScreen();
        cout << "===========================\n";
        cout << "         REGISTER\n";
        cout << "===========================\n";
        cout << "Enter username: ";
        cin >> newUser .username;
        cout << "Enter password: ";
        cin >> newUser .password;
        cin.ignore();
        cout << "Enter Full Name (First Middle Last): ";
        getline(cin, newUser .fullName);
        cout << "Enter Program: ";
        getline(cin, newUser .program);
        cout << "Enter Section: ";
        getline(cin, newUser .section);
        cout << "Enter Gender (Male/Female): ";
        getline(cin, newUser .gender);
        cout << "WRITE YOUR BIRTHDAY LIKE THIS -> 2009-11-20" << endl;
        cout << "Enter Birthday (YYYY-MM-DD): ";
        getline(cin, newUser .birthday);
        cout << "Enter Year Level: ";
        cin >> newUser .yearLevel;

        ofstream outFile("accounts.txt", ios::app);
        if (outFile.is_open()) {
            outFile << newUser .username << ","
                    << newUser .password << ","
                    << newUser .fullName << ","
                    << newUser .program << ","
                    << newUser .section << ","
                    << newUser .gender << ","
                    << newUser .birthday << ","
                    << newUser .yearLevel << endl;
            outFile.close();
            cout << "Registration successful!\n";
            Sleep(300);
        } else {
            cout << "Error: Unable to open file!\n";
        }
    }

    void viewAllStudents() {
        clearScreen();
        ifstream inFile("accounts.txt");
        if (!inFile.is_open()) {
            cout << "Error: Unable to open accounts file!\n";
            return;
        }

        string line;
        cout << "========================\n";
        cout << "   STUDENTS SCORES" << endl;
        cout << "========================\n";
        while (getline(inFile, line)) {
            size_t pos = 0;
            string data[8];
            int i = 0;

            while ((pos = line.find(",")) != string::npos && i < 7) {
                data[i] = line.substr(0, pos);
                line.erase(0, pos + 1);
                i++;
            }
            data[i] = line;

            cout << "\nSTUDENT \033[38;5;213m" << data[0] << "\033[38;5;214m\n\n";
            cout << "Username: \033[38;5;213m" << data[0] << "\033[38;5;214m\n";
            cout << "Full Name: \033[38;5;213m" << data[2] << "\033[38;5;214m\n";
            cout << "Program & Section: \033[38;5;213m" << data[3] + "\033[38;5;214m - \033[38;5;213m" + data[4] << "\033[38;5;214m\n";
            cout << "Gender: \033[38;5;213m" << data[5] << "\033[38;5;214m\n";
            cout << "Birthday: \033[38;5;213m" << data[6] << "\033[38;5;214m\n";
            cout << "Year Level: \033[38;5;213m" << data[7] << "\033[38;5;214m\n";

            string progressFileName = data[0] + "_progress.txt";
            ifstream progressFile(progressFileName);
            cout << "Progress \n";

            if (!progressFile.is_open()) {
                cout << "No progress found for " << data[0] << "\n";
            } else {
                string progressLine;
                while (getline(progressFile, progressLine)) {
                    if (progressLine.find("-1") != string::npos) {
                        size_t pos = 0;
                        while ((pos = progressLine.find("-1")) != string::npos) {
                            progressLine.replace(pos, 2, "\033[31mNot Answered\033[38;5;214m");
                        }
                    }
                    cout << progressLine << "\n";
                }
                progressFile.close();
            }

            cout << "\n";
            cout << "-----------------\n";
        }

        inFile.close();
        system("pause");
    }


    void Developers(){
    clearScreen();
    cout << "||======================================================================||" << endl;
    cout << "||                                                                      ||" << endl;
    cout << "||                                                                      ||" << endl;
    cout << "||                                                                      ||" << endl;
