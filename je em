    cout << "                             \\$$$$$$ / \\$$$$$$  |$$ |$$$$$$$$\\ \033[38;5;130m$$ |  $$ |\\$$$$$$  |$$$$$$$  |\033[0m" << endl;
    cout << "                              \\___$$$\\  \\______/ \\__|\\________|\033[38;5;130m\\__|  \\__| \\______/ \\_______/\033[0m" << endl;
    cout << "                                  \\___|                                                               " << endl;
    cout << "                                                                                                       " << endl;
    cout << "                                                                                                       " << endl;
    cout << "                                                                                                       " << endl;
    cout << "                                                                                                       " << endl;
    cout << "                                                   \033[38;5;213mLOADING COMPLETE!\033[0m" << endl;
    cout << "                                                                                                       " << endl;
    cout << "                         =====================================================================" << endl;
    Sleep(60);
    system("pause");
    return;
}
