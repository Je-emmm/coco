}},
{"ESP", {
{"1. What is the core value of ESP? (a) Honesty (b) Greed (c) Anger", 'a'},
{"2. Respect is important because? (a) It shows care (b) It makes you rich (c) It is optional", 'a'}
}}
};

        if (quizQuestions.find(subject) == quizQuestions.end()) {
        cout << "Invalid subject!\n";
        return;
    }

    clearScreen();
    cout << "\nTaking " << subject << " Quiz...\n";

    int score = 0;
    char answer;

    for (size_t i = 0; i < quizQuestions[subject].size(); i++) {
        cout << quizQuestions[subject][i].first << "\n";
        cout << "Your answer: ";
        cin >> answer;

        if (userAnswers.find(subject) == userAnswers.end()) {
            userAnswers[subject] = vector<char>(quizQuestions[subject].size(), '\0');
        }
        userAnswers[subject][i] = tolower(answer);

        if (tolower(answer) == quizQuestions[subject][i].second) {
            score++;
        }
    }

    cout << "Quiz completed! Your score: " << score << "/" << quizQuestions[subject].size() << "\n";

    quizScores[subject] = score;
    saveProgress();
    saveAnswersToFile();
    system("pause");
}


    void viewAccountInformation() {
        clearScreen();
        int year, month, day, choice1;
        sscanf(currentUser .birthday.c_str(), "%d-%d-%d", &year, &month, &day);
        time_t t = time(0);
        tm* now = localtime(&t);
        int age = now->tm_year + 1900 - year;
        if (now->tm_mon + 1 < month || (now->tm_mon + 1 == month && now->tm_mday < day)) {
            age--;
        }
        cout << "\t\t\033[30;101m||======================================================================||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m   ACCOUNT INFORMATION                            \033[101m||" << endl;
    cout << "\t\t||\033[107m--------------------\033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m Fullname                 Username                \033[101m||" << endl;
    cout << "\t\t||\033[107m  1. Back           \033[30;47m " << setw(23) << left << currentUser .fullName << setw(26) << left << currentUser .username << "\033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m--------------------\033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m Program                  Section                 \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m " << setw(23) << left << currentUser .program << setw(26) << left << currentUser .section << "\033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m Age                      Birthday                \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m " << setw(23) << left << age << setw(26) << left << currentUser .birthday << "\033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m Gender                                           \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m " << setw(44) << left << currentUser .gender << "     \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||======================================================================||\033[0m" << endl;
    cout << "Enter your choice :: ";
    cin >> choice1;

        if(choice1 == '1'){
            return;
        }
        system("pause");
    }

    void changePassword() {
        clearScreen();
        string oldPassword, newPassword;
        cout << "===========================\n";
        cout << " CHANGE PASSWORD\n";
        cout << "===========================\n";
        cout << "Enter current password: ";
        cin >> oldPassword;

        if (oldPassword == currentUser .password) {
            cout << "Enter new password: ";
            cin >> newPassword;
            currentUser .password = newPassword;
            ifstream inFile("accounts.txt");
            ofstream outFile("temp.txt");
            string line;
            while (getline(inFile, line)) {
                if (line.find(currentUser .username) != string::npos) {
                    size_t pos = 0;
                    string data[8];
                    int i = 0;
                    while ((pos = line.find(",")) != string::npos && i < 7) {
                        data[i] = line.substr(0, pos);
                        line.erase(0, pos + 1);
                        i++;
                    }
                    data[i] = line;
                    data[1] = newPassword;
                    outFile << data[0] << "," << data[1] << "," << data[2] << "," << data[3] << ","
                            << data[4] << "," << data[5] << "," << data[6] << "," << data[7] << endl;
                } else {
                    outFile << line << endl;
                }
            }
            inFile.close();
            outFile.close();
            remove("accounts.txt");
            rename("temp.txt", "accounts.txt");
            cout << "Password changed successfully!\n";
        } else {
            cout << "Incorrect current password!\n";
        }
        system("pause");
    }
};

