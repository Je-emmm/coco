    cout << "Enter your Choice :: ";
                        cin >> subChoice;
                        if (subChoice == 1) quizSys.showProgress();
                        else if (subChoice == 2) {
                            string subjects[] = {"Ethics", "PURPOSIVE_COMMUNICATION", "ESP"};
                            int quizNum;
                            cout << "\nChoose Quiz (1. Ethics, 2. PURPOSIVE_COMMUNICATION, 3. ESP): ";
                            cin >> quizNum;
                            if (quizNum >= 1 && quizNum <= 3) {
                                quizSys.takeQuiz(subjects[quizNum - 1]);
                            } else {
                                cout << "Invalid\n";
                                system("pause");
                            }
                        } else if (subChoice == 3) {
                            quizSys.viewAccountInformation();
                        } else if (subChoice == 4) {
                            quizSys.changePassword();
                        } else if (subChoice == 5) {break;
                        } else if (subChoice == 6){
                            quizSys.Developers();
                        }
                    }
                }
            }
        } else if (choice == 3) break;
    }
    return 0;
}

