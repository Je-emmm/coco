cout << "\t\t||\033[107m    ---------------        ---------------        -----------------   \033[101m||" << endl;
cout << "\t\t||\033[107m                             5. Log Out                               \033[101m||" << endl;
cout << "\t\t||\033[107m    ---------------        ---------------        -----------------   \033[101m||" << endl;
cout << "\t\t||\033[107m    3. View Account                               4. Change Password  \033[101m||" << endl;
cout << "\t\t||\033[107m    ---------------                               -----------------   \033[101m||" << endl;
cout << "\t\t||\033[107m                                                                      \033[101m||" << endl;
