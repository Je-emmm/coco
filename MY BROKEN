                        if (subChoice == 1) quizSys.viewAllStudents();
                        else if (subChoice == 2) break;
                    }
                } else {
                    while (true) {
                        quizSys.clearScreen();
                        cout << "\t\t\033[30;101m||======================================================================||" << endl;
cout << "\t\t||\033[107m                                                                      \033[101m||" << endl;
cout << "\t\t||\033[107m     Welcome to QuizHub                                               \033[101m||" << endl;
