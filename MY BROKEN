        }
        Sleep(1000);


        if (username == "admin" && password == "adminpass") {
            currentUser .username = username;
            currentUser .password = password;
            currentUser .fullName = "Administrator";
            currentUser .program = "Admin Program";
            currentUser .section = "Admin Section";
            progressFile = "admin_progress.txt";
            return true;
        } else {
            cout << "Invalid username or password!" << endl;
        }
        ifstream inFile("accounts.txt");
        if (!inFile.is_open()) {
            cout << "Error: Unable to open file!\n";
            return false;
        }
        while (getline(inFile, line)) {
            size_t pos = 0;
            string data[8];
            int i = 0;
            while ((pos = line.find(",")) != string::npos && i < 7) {
                data[i] = line.substr(0, pos);
                line.erase(0, pos + 1);
                i++;
            }
            data[i] = line;
            if (data[0] == username && data[1] == password) {
                currentUser .username = data[0];
                currentUser .password = data[1];
                currentUser .fullName = data[2];
                currentUser .program = data[3];
                currentUser .section = data[4];
                currentUser .gender = data[5];
                currentUser .birthday = data[6];
                currentUser .yearLevel = stoi(data[7]);
                inFile.close();
                progressFile = currentUser .username + "_progress.txt";
                loadProgress();
                return true;
            }
        }
        inFile.close();
        return false;
    }




    void loadProgress() {
    quizScores.clear();
    string subjects[] = {"Applied_Ethics", "Purposive_Communication", "NSTP_2", "Discrete_Mathematics", "PATHFIT_2", "FILDIS", "STS", "Art_Appreciation", "Computer_Programming"};

    for (const string& subject : subjects) {
        string subjectFileName = currentUser .username + "_" + subject + "_progress.txt";
        ifstream inFile(subjectFileName);
        if (inFile.is_open()) {
            int score;
            inFile >> score;
            quizScores[subject] = score;
            inFile.close();
        } else {
            quizScores[subject] = -1;
        }
    }
}

   void loadAnswersFromFile(const string& username) {
    ifstream inputFile(username + "_answers.txt");
    if (!inputFile) {
        cerr << "Error: Could not open file " << username + "_answers.txt" << endl;
        return;
    }

    string subject;
    int questionIndex;
    char answer;

    userAnswers.clear();

    while (inputFile >> subject >> questionIndex >> answer) {
        if (userAnswers[subject].size() <= questionIndex) {
            userAnswers[subject].resize(questionIndex + 1);
        }
        userAnswers[subject][questionIndex] = answer;
    }
    inputFile.close();
}

void saveAnswersToFile() {
    ofstream file(currentUser .username + "_answers.txt");
    if (file.is_open()) {
        for (const auto& subject : userAnswers) {
            for (size_t i = 0; i < subject.second.size(); ++i) {
                if (subject.second[i] != '\0') {
                    file << subject.first << " " << i << " " << subject.second[i] << "\n";
                }
            }
        }
        file.close();
    } else {
        cerr << "Error: Unable to open file for writing!" << endl;
    }
}
    void saveProgress() {
    for (const auto& entry : quizScores) {
        string subjectFileName = currentUser .username + "_" + entry.first + "_progress.txt";
        ofstream outFile(subjectFileName);
        if (outFile.is_open()) {
            outFile << entry.second << endl;
            outFile.close();
        } else {
            cerr << "Error: Unable to open file for writing progress for " << entry.first << endl;
        }
    }
}

    void showProgress() {
    loadProgress();
    int i = 1;

    clearScreen();
    cout << "\t\t\033[30;101m||======================================================================||" << endl;
    cout << "\t\t||\033[107m                         QUIZ PROGRESS REPORT                         \033[101m||" << endl;
    cout << "\t\t||\033[107m----------------------------------------------------------------------\033[101m||" << endl;
    cout << "\t\t||\033[107m Fullname: " << setw(45) << left << currentUser .fullName << "    \033[48;5;13m Press 10 \033[101m||" << endl;
    cout << "\t\t||\033[107m Program & Section: " << setw(40) << left << (currentUser .program + " - " + currentUser .section) << "\033[48;5;13m To Home  \033[101m||" << endl;
    cout << "\t\t||\033[107m----------------------------------------------------------------------\033[101m||" << endl;
    cout << "\t\t||\033[107mSubject                       | Score  |    Percentage    | Status    \033[101m||" << endl;
    cout << "\t\t||\033[107m----------------------------------------------------------------------\033[101m||" << endl;

    int totalScore = 0, totalPossible = 0;
    string subjects[] = {"Applied_Ethics", "Purposive_Communication", "NSTP_2", "Discrete_Mathematics", "PATHFIT_2", "FILDIS", "STS", "Art_Appreciation", "Computer_Programming"};

    for (const string& subject : subjects) {
        string status;
        int score = quizScores.count(subject) ? quizScores[subject] : -1;

        double percentage = 50;
        if (quizQuestions.find(subject) != quizQuestions.end()) {
            int possibleScore = quizQuestions[subject].size();
            percentage = (possibleScore > 0) ? 50 + (static_cast<double>(score) / possibleScore) * 50 : 0;

            if (score == -1) {
                status = "NOT TAKEN";
            } else if (score == 0) {
                status = "\033[30;41mFAILED";
            } else {
                status = (percentage >= 75) ? "\033[30;42m PASSED  \033[107m" : "\033[30;41m FAILED  \033[107m";
                totalScore += score;
                totalPossible += possibleScore;
            }

            cout << "\t\t||\033[107m" << setw(1) << left << i << ". " << setw(27) << left << subject
                 << "| " << setw(7) << left << (score >= 0 ? to_string(score) + "/" + to_string(possibleScore) : "N/A")
                 << "| " << setw(17) << left << (score >= 0 ? to_string(percentage) + "%" : "N/A")
                 << "| " << setw(9) << left << status << " \033[101m||" << endl;
            cout << "\t\t||\033[107m----------------------------------------------------------------------\033[101m||" << endl;
            i++;
        } else {
            cout << "\t\t||\033[107m" << setw(1) << left << i << ". " << setw(27) << left << subject
                 << "| " << setw(7) << left << "N/A"
                 << "| " << setw(17) << left << "N/A"
                 << "| " << setw(9) << left << "NOT TAKEN" << " \033[101m||" << endl;
            cout << "\t\t||\033[107m----------------------------------------------------------------------\033[101m||" << endl;
            i++;
        }
    }

    double overallPercentage = (totalPossible > 0) ? 50 + (static_cast<double>(totalScore) / totalPossible) * 50 : 0;
    string overallStatus = (overallPercentage >= 75) ? "\033[30;42m PASSED \033[107m" : "\033[30;41m FAILED \033[107m";
    cout << "\t\t||\033[107mTotal Score: " << totalScore << "/" << totalPossible
         << " | Overall Percentage: " << fixed << setprecision(2) << overallPercentage << "%";
    if (totalPossible > 0) {
        cout << " | " << setw(18) <<right<< overallStatus << "            \033[101m||" << endl;
    } else {
        cout << " | " << setw(18) << "N/A" << "     \033[101m||" << endl;
    }
    cout << "\t\t||======================================================================||" << endl;
    cout << "\t\t\033[0m";
    int review;
    cout << "Choose what Subject you want to Review. :: ";
    cin >> review;
    if (review == 10){
    return;
    }
    if(review >= 1 || review <= 9){
        reviewQuiz(subjects[review - 1]);
    }

    system("pause");
}







    void reviewQuiz(const string& subject) {
        if (quizQuestions.find(subject) == quizQuestions.end()) {
                clearScreen();
            cout << "\n\n\n\n\n\n\n\t\t\t\033[30;101m||==========================================================================||\033[0m" << endl;
            cout << "\t\t\t\033[30;101m||\033[107m                                                                          \033[101m||\033[0m" <<endl;
            cout << "\t\t\t\033[30;101m||\033[107m                            INVALID SUBJECT                               \033[101m||\033[0m"<<endl;
            cout << "\t\t\t\033[30;101m||\033[107m                                                                          \033[101m||\033[0m" <<endl;
            cout << "\t\t\t\033[30;101m||==========================================================================||\033[0m" << endl;
            return;
        }

        clearScreen();
        cout << "\nReviewing " << subject << " Quiz...\n";

        for (size_t i = 0; i < quizQuestions[subject].size(); ++i) {
            const auto& q = quizQuestions[subject][i];
            cout << q.first << "\n";
            char correctAnswer = q.second;

            char userAnswer = (userAnswers.find(subject) != userAnswers.end() && userAnswers[subject].size() > i)
                              ? userAnswers[subject][i]
                              : '\0';

            if (userAnswer == '\0') {
                cout << "Your answer: Not attempted\n";
            } else {
                cout << "Your answer: " << userAnswer << "\n";
                cout << "Correct answer is: " << correctAnswer << "\n";
                if (userAnswer == correctAnswer) {
                    cout << "Feedback: Correct!\n";
                } else {
                    cout << "Feedback: Incorrect!\n";
                }
            }

            cout << "-----------------------------------\n";
        }

        cout << "End of review for " << subject << " Quiz.\n";
        system("pause");
    }








    void choosequiz(){
        int i = 1;
        string subjects[] = {"Applied_Ethics", "Purposive_Communication", "NSTP_2", "Discrete_Mathematics", "PATHFIT_2", "FILDIS", "STS", "Art_Appreciation", "Computer_Programming"};
                            int quizNum;
                            clearScreen();
                            cout << "\t\t\033[30;101m                                                              "<<endl;
                            cout << "\t\t  \033[107m                     Choose Quiz                          \033[101m  "<<endl;
                            cout << "\t\t  \033[107m==========================================================\033[101m  "<<endl;
                            int totalScore = 0, totalPossible = 0;

    for (const string& subject : subjects) {
        string status;
        int score = quizScores.count(subject) ? quizScores[subject] : -1;


        if (quizQuestions.find(subject) != quizQuestions.end()) {
            int possibleScore = quizQuestions[subject].size();
            double percentage = (possibleScore > 0) ? (static_cast<double>(score) / possibleScore) * 100 : 0;

            if (score == -1) {
                status = "NOT TAKEN";
            } else if (score == 0) {
                status = "TAKEN";
            } else {
                status = (percentage >= 75) ? "TAKEN" : "TAKEN";
                totalScore += score;
                totalPossible += possibleScore;
            }

            cout << "\t\t  \033[107m" << setw(1) << left << i << ". "<< setw(43)<< subject
                 << "| " << setw(9) << left << status << " \033[101m  " << endl;
            cout << "\t\t  \033[107m----------------------------------------------------------\033[101m  " << endl;
            i++;
        } else {
            cout << "\t\t  \033[107m" << setw(1) << left << i << ". " << setw(43) << left << subject << "| "
    << setw(9) << left << "NOT TAKEN" << " \033[101m  " << endl;
            cout << "\t\t  \033[107m----------------------------------------------------------\033[101m  " << endl;
            i++;
        }
    }
    cout << "\t\t  \033[107m10.Exit                                                   \033[101m  "<<endl;
    cout << "\t\t                                                              " << endl;
    cout << "\t\t\033[0m";

                                cin >> quizNum;
                            if (quizNum >= 1 && quizNum <= 9) {
                                takeQuiz(subjects[quizNum - 1]);
                            } else if(quizNum == 10) {
                                return;

                            }



    }

void takeQuiz(string subject) {
    if (quizQuestions.find(subject) == quizQuestions.end()) {
        cout << "Invalid subject!\n";
        system("pause");
        return;
    }

    if (quizScores.find(subject) != quizScores.end() && quizScores[subject] != -1) {
        if (subject == "Applied_Ethics"){
            clearScreen();
            cout << "\n\n\n\n\n\t\t\033[30;101m||==========================================================================||" << endl;
            cout << "\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t||\033[107m    You have already answered the Applied Ethics quiz.                    \033[101m||"<<endl;
            cout << "\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t||==========================================================================||\033[0m" << endl;
        system("pause");
        return;
        }else if (subject == "Purposive_Communication"){
            clearScreen();
            cout << "\n\n\n\n\n\t\t\033[30;101m||==========================================================================||" << endl;
            cout << "\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t||\033[107m    You have already answered the Purposive Communication quiz.           \033[101m||"<<endl;
            cout << "\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t||==========================================================================||\033[0m" << endl;

        system("pause");
        return;
        }else if (subject == "NSTP_2"){
            clearScreen();
            cout << "\n\n\n\n\n\t\t\033[30;101m||==========================================================================||" << endl;
            cout << "\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t||\033[107m    You have already answered the NSTP 2 quiz.                            \033[101m||"<<endl;
            cout << "\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t||==========================================================================||\033[0m" << endl;

        system("pause");
