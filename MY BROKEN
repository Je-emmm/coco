        system("pause");
        return;


        }else if (subject == "FILDIS"){
            clearScreen();
            cout << "\n\n\n\n\n\t\t\033[30;101m||==========================================================================||" << endl;
            cout << "\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t||\033[107m    You have already answered the FILDIS quiz.                            \033[101m||"<<endl;
            cout << "\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t||==========================================================================||\033[0m" << endl;

        system("pause");
        return;


        }else if (subject == "STS"){
            clearScreen();
            cout << "\n\n\n\n\n\t\t\033[30;101m||==========================================================================||" << endl;
            cout << "\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t||\033[107m    You have already answered the Science Technology And Society quiz.    \033[101m||"<<endl;
            cout << "\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t||==========================================================================||\033[0m" << endl;
        system("pause");
        return;


        }else if (subject == "Art_Appreciation"){
            clearScreen();
            cout << "\n\n\n\n\n\t\t\033[30;101m||==========================================================================||" << endl;
            cout << "\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t||\033[107m    You have already answered the Art Appreciation quiz.                  \033[101m||"<<endl;
            cout << "\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t||==========================================================================||\033[0m" << endl;

        system("pause");
        return;
        }else if (subject == "Computer_Programming"){
            clearScreen();
            cout << "\n\n\n\n\n\t\t\033[30;101m||==========================================================================||" << endl;
            cout << "\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t||\033[107m    You have already answered the Computer Programming quiz.              \033[101m||"<<endl;
            cout << "\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t||==========================================================================||\033[0m" << endl;

        system("pause");
        return;
        }
    }

    clearScreen();
    cout << "\nTaking " << subject << " Quiz...\n\n";
    cout << "BE CAREFUL WHEN ANSWERING YOU CANNOT TURN BACK."<<endl;

    int score = 0;
    char answer;

    userAnswers[subject].resize(quizQuestions[subject].size(), '\0');

    for (size_t i = 0; i < quizQuestions[subject].size(); i++) {
        cout << quizQuestions[subject][i].first << "\n";
        cout << "Your answer: ";
        cin >> answer;

        userAnswers[subject][i] = tolower(answer);

        if (tolower(answer) == quizQuestions[subject][i].second) {
            score++;
        }
    }

    cout << "Quiz completed! Your score: " << score << "/" << quizQuestions[subject].size() << "\n";

    quizScores[subject] = score;
    saveProgress();
    saveAnswersToFile();
    system("pause");
}




    void viewAccountInformation() {
        clearScreen();
        int year, month, day, choice1;
        sscanf(currentUser .birthday.c_str(), "%d-%d-%d", &year, &month, &day);
        time_t t = time(0);
        tm* now = localtime(&t);
        int age = now->tm_year + 1900 - year;
        if (now->tm_mon + 1 < month || (now->tm_mon + 1 == month && now->tm_mday < day)) {
            age--;
        }
        cout << "\t\t\033[30;101m||======================================================================||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m   ACCOUNT INFORMATION                            \033[101m||" << endl;
    cout << "\t\t||\033[107m--------------------\033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m Fullname                          Username       \033[101m||" << endl;
    cout << "\t\t||\033[107m  1. Back           \033[30;47m " << setw(34) << left << currentUser .fullName << setw(15) << left << currentUser .username << "\033[30;101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m--------------------\033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m Program                           Section        \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m " << setw(34) << left << currentUser .program << setw(15) << left << currentUser .section << "\033[30;101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m Age                                Birthday      \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m " << setw(34) << left << age << setw(15) << left << currentUser .birthday << "\033[30;101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m Gender                                           \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m " << setw(44) << left << currentUser .gender << "     \033[30;101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||\033[107m                    \033[30;47m                                                  \033[101m||" << endl;
    cout << "\t\t||======================================================================||\033[0m" << endl;
    cout << "Enter your choice :: ";
    cin >> choice1;

        if(choice1 == '1'){
            return;
        }
        system("pause");
    }




    void changePassword() {
        clearScreen();
        string oldPassword, newPassword;
        cout << "\t\t\t\033[30;101m|=============================================================||\033[0m" << endl;
        cout << "\t\t\t\033[30;101m||\033[30;107m                      CHANGE PASSWORD                       \033[30;101m||\033[0m" << endl;
        cout << "\t\t\t\033[30;101m||\033[30;107m -----------------------------------------------------------\033[30;101m||\033[0m" << endl;
        cout << "\t\t\t\033[30;101m||\033[30;107m                                                            \033[30;101m||\033[0m" << endl;
        cout << "\t\t\t\033[30;101m||\033[30;107m    Current Password:    ""                                   \033[30;101m||\033[0m" << endl;
        cout << "\t\t\t\033[30;101m||\033[30;107m                                                            \033[30;101m||\033[0m" << endl;
        cout << "\t\t\t\033[30;101m||\033[30;107m    New Password:    ""                                       \033[30;101m||\033[0m" << endl;
        cout << "\t\t\t\033[30;101m||\033[30;107m                                                            \033[30;101m||\033[0m" << endl;
        cout << "\t\t\t\033[30;101m||\033[30;107m                                                            \033[30;101m||\033[0m" << endl;
        cout << "\t\t\t\033[30;101m||============================================================||\033[0m" << endl;
        cout << "Enter current password: ";
        cin >> oldPassword;

        if (oldPassword == currentUser .password) {
            clearScreen();
            cout << "\t\t\t\033[30;101m|=============================================================||\033[0m" << endl;
            cout << "\t\t\t\033[30;101m||\033[30;107m                      CHANGE PASSWORD                       \033[30;101m||\033[0m" << endl;
            cout << "\t\t\t\033[30;101m||\033[30;107m -----------------------------------------------------------\033[30;101m||\033[0m" << endl;
            cout << "\t\t\t\033[30;101m||\033[30;107m                                                            \033[30;101m||\033[0m" << endl;
            cout << "\t\t\t\033[30;101m||\033[30;107m    Current Password:    "<<setw(35) << left << oldPassword<<"\033[30;101m||\033[0m" << endl;
            cout << "\t\t\t\033[30;101m||\033[30;107m                                                            \033[30;101m||\033[0m" << endl;
            cout << "\t\t\t\033[30;101m||\033[30;107m    New Password:    ""                                       \033[30;101m||\033[0m" << endl;
            cout << "\t\t\t\033[30;101m||\033[30;107m                                                            \033[30;101m||\033[0m" << endl;
            cout << "\t\t\t\033[30;101m||\033[30;107m                                                            \033[30;101m||\033[0m" << endl;
            cout << "\t\t\t\033[30;101m||============================================================||\033[0m" << endl;
            cout << "Enter new password: ";
            cin >> newPassword;
            currentUser .password = newPassword;
                clearScreen();
                cout << "\t\t\t\033[30;101m|=============================================================||\033[0m" << endl;
                cout << "\t\t\t\033[30;101m||\033[30;107m                      CHANGE PASSWORD                       \033[30;101m||\033[0m" << endl;
                cout << "\t\t\t\033[30;101m||\033[30;107m -----------------------------------------------------------\033[30;101m||\033[0m" << endl;
                cout << "\t\t\t\033[30;101m||\033[30;107m                                                            \033[30;101m||\033[0m" << endl;
                cout << "\t\t\t\033[30;101m||\033[30;107m    Current Password:    "<<setw(35) << left << oldPassword<<"\033[30;101m||\033[0m" << endl;
                cout << "\t\t\t\033[30;101m||\033[30;107m                                                            \033[30;101m||\033[0m" << endl;
                cout << "\t\t\t\033[30;101m||\033[30;107m    New Password:    "<<setw(35) << left << newPassword<<"    \033[30;101m||\033[0m" << endl;
                cout << "\t\t\t\033[30;101m||\033[30;107m                                                            \033[30;101m||\033[0m" << endl;
                cout << "\t\t\t\033[30;101m||\033[30;107m                                                            \033[30;101m||\033[0m" << endl;
                cout << "\t\t\t\033[30;101m||============================================================||\033[0m" << endl;
            ifstream inFile("accounts.txt");
            ofstream outFile("temp.txt");
            string line;
            while (getline(inFile, line)) {
                if (line.find(currentUser .username) != string::npos) {
                    size_t pos = 0;
                    string data[8];
                    int i = 0;
                    while ((pos = line.find(",")) != string::npos && i < 7) {
                        data[i] = line.substr(0, pos);
                        line.erase(0, pos + 1);
                        i++;
                    }
                    data[i] = line;
                    data[1] = newPassword;
                    outFile << data[0] << "," << data[1] << "," << data[2] << "," << data[3] << ","
                            << data[4] << "," << data[5] << "," << data[6] << "," << data[7] << endl;
                } else {
                    outFile << line << endl;
                }
            }
            inFile.close();
            outFile.close();
            remove("accounts.txt");
            rename("temp.txt", "accounts.txt");
            cout << "Password changed successfully!\n";
        } else {
            clearScreen();
            cout << "\n\n\n\n\n\n\n\t\t\t\033[30;101m||==========================================================================||" << endl;
            cout << "\t\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t\t||\033[107m                            WRONG PASSWORD!                               \033[101m||"<<endl;
            cout << "\t\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t\t||==========================================================================||\033[0m" << endl;
            system("pause");
            changePassword();
        }
        system("pause");
    }
};

