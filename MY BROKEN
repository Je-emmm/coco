            cout << "\t\t\t||==========================================================================||\033[0m" << endl;
            system("pause");
            return;
        } else {
            cout << "Error: Unable to open file!\n";
        }
    }

    void viewAllStudents() {
    clearScreen();
    ifstream inFile("accounts.txt");
    if (!inFile.is_open()) {
        cout << "Error: Unable to open accounts file!\n";
        return;
    }

    string line;
    int studentnumber = 1;
    string subjects[] = {"Applied_Ethics", "Purposive_Communication", "NSTP_2", "Discrete_Mathematics", "PATHFIT_2", "FILDIS", "STS", "Art_Appreciation", "Computer_Programming"};

    while (getline(inFile, line)) {
        size_t pos = 0;
        string data[8];
        int i = 0;

        while ((pos = line.find(",")) != string::npos && i < 7) {
            data[i] = line.substr(0, pos);
            line.erase(0, pos + 1);
            i++;
        }
        data[i] = line;

        cout << "\t\t\033[38;5;235m\033[48;5;235m||=============================================================================||\033[0m" << endl;
        cout << "\t\t\033[38;5;235m\033[48;5;235m||                  \033[97mSTUDENT: " << setw(7) << right << setfill('0') << studentnumber << "   Account Information                     \033[38;5;235m  \033[0m" << endl;
        cout << "\t\t\033[38;5;235m\033[48;5;235m||                                                                             ||\033[0m" << endl;
        cout << "\t\t\033[38;5;235m\033[48;5;235m|| \033[48;5;234m                                                                     \033[48;5;235m       ||\033[0m" << endl;
        cout << "\t\t\033[38;5;235m\033[48;5;235m|| \033[48;5;234m  \033[90mName:     \033[97m" << setw(57) << setfill(' ') << left << data[2] << "\033[48;5;235m         \033[0m" << endl;
        cout << "\t\t\033[38;5;235m\033[48;5;235m|| \033[48;5;234m  \033[90mUsername: \033[97m" << setw(57) << left << data[0] << "\033[48;5;235m         \033[0m" << endl;
        cout << "\t\t\033[38;5;235m\033[48;5;235m|| \033[48;5;234m  \033[90mGender:   \033[97m" << setw(57) << left << data[5] << "\033[48;5;235m         \033[0m" << endl;
        cout << "\t\t\033[38;5;235m\033[48;5;235m|| \033[48;5;234m  \033[90mProgram & Section: \033[97m" << setw(48) << left << data[3] + " - " + data[4] << "\033[48;5;235m         \033[0m" << endl;
        cout << "\t\t\033[38;5;235m\033[48;5;235m|| \033[48;5;234m  \033[90mYear Level: \033[97m" << setw(55) << left << data[6] << "\033[48;5;235m         \033[0m" << endl;
        cout << "\t\t\033[38;5;235m\033[48;5;235m||=============================================================================||\033[0m" << endl;

        int totalPossibleScore = 0;
        int totalScore = 0;
        string totalScoreString = "N/A";
        string overallScoreString = "N/A";
        string overallStatusString = "N/A";

        for (const string& subject : subjects) {
            string progressFileName = data[0] + "_" + subject + "_progress.txt";
            ifstream progressFile(progressFileName);
            int score = -1;

            if (progressFile.is_open()) {
                progressFile >> score;
                progressFile.close();
            }

            totalPossibleScore += 25;
            totalScore += score;

            string scoreString;
            int scoreint;
            string status;
            string color;
            if (score == -1) {
                scoreString = "N/A";
                status = "N/A";
                color = "\033[97m";
            } else if (score >= 13) {
                scoreint = score;
                status = "PASSED";
                color = "\033[32m";
            } else {
                scoreint = score;
                status = "FAILED";
                color = "\033[31m";
            }
        if (score == -1){
            cout << "\t\t\033[38;5;235m\033[48;5;235m|| \033[48;5;234m  \033[90m"<<setw(25) <<left << subject << ": \033[97m"
                <<setw(5)<< left << scoreString << "   " << color<<setw(32)<<left << status  <<"\033[0m\033[48;5;235m         \033[0m" << endl;

                }else if (score >= 1 || score <= 25){
            cout << "\t\t\033[38;5;235m\033[48;5;235m|| \033[48;5;234m  \033[90m"<<setw(25) <<left << subject << ": \033[97m"
                <<setw(5)<< left << scoreint << "   " << color <<setw(32) <<left<< status << "\033[0m\033[48;5;235m         \033[0m" << endl;
                }
        }
        double overallScore = (totalPossibleScore > 0) ? (50 + (static_cast<double>(totalScore) / totalPossibleScore) * 50) : 0;
        string overallStatus = overallScore >= 75 ? "PASSED" : "FAILED";
        string overallColor = overallScore >= 75 ? "\033[32m" : "\033[31m";

        if(totalScore <= -1){
        cout << "\t\t\033[38;5;235m\033[48;5;235m|| \033[48;5;234m                                                                     \033[48;5;235m       ||\033[0m" << endl;
        cout << "\t\t\033[38;5;235m\033[48;5;235m|| \033[48;5;234m  \033[90mOverall Score: \033[97m"
              << totalScoreString << " ("
             << fixed << setprecision(2) << overallScoreString << "%)  "
             << overallStatusString << "                                     \033[0m\033[48;5;235m         \033[0m" << endl;
        cout << "\t\t\033[38;5;235m\033[48;5;235m||=============================================================================||\033[0m" << endl;
        studentnumber++;
        }else if (totalScore > 0){
        cout << "\t\t\033[38;5;235m\033[48;5;235m|| \033[48;5;234m                                                                     \033[48;5;235m       ||\033[0m" << endl;
        cout << "\t\t\033[38;5;235m\033[48;5;235m|| \033[48;5;234m  "<<setw(10)<< left <<"\033[90mOverall Score: \033[97m"
             << overallColor <<setw(3) <<left << totalScore << "/"<<setw(4)<<left << totalPossibleScore << " ("
             << fixed << setprecision(2) << overallScore << "%)  "
             << overallStatus << "                           \033[0m\033[48;5;235m         \033[0m" << endl;
        cout << "\t\t\033[38;5;235m\033[48;5;235m||=============================================================================||\033[0m" << endl;
        }
        cout <<"\n"<<endl;
    }
    inFile.close();
    system("pause");
}


    bool loginUser () {
