public:
    User getCurrentUser () const {
        return currentUser ;
    }

    void simpleLoading() {
        cout << "" << endl;
    }

    void clearScreen() {
        system("CLS");
    }

    void registerUser () {
        User newUser ;
        clearScreen();
        cout << "\t\t\t\033[30;107m||============================================================||" << endl;
        cout << "\t\t\t\033[30;107m||                   REGISTERING ACCOUNTS                     ||" << endl;
        cout << "\t\t\t\033[30;107m||------------------------------------------------------------||" << endl;
        cout << "\t\t\t\033[30;107m||                                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||   Username:    ""                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||                                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||   Password:    ""                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||                                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||   Fullname:    ""                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||                                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||   Program:     ""                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||                                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||   Section:     ""                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||                                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||   Gender:      ""                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||                                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||   Birthday:    ""                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||                                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||   YearLevel:   ""                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||                                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||                                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||                                                            ||" << endl;
        cout << "\t\t\t\033[30;107m||============================================================||\033[0m" << endl;
        cout << "\t\t\tEnter username: ";
        cin >> newUser .username;
        if(newUser .username == "register" || newUser .username == "admin" || newUser .username == "REGISTER" ){
            clearScreen();
            cout << "\n\n\n\n\n\n\n\t\t\t\033[30;101m||==========================================================================||" << endl;
            cout << "\t\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t\t||\033[107m                             INVALID USERNAME!                            \033[101m||"<<endl;
            cout << "\t\t\t||\033[107m                                                                          \033[101m||" <<endl;
            cout << "\t\t\t||==========================================================================||\033[0m" << endl;
            registerUser();
        }
        clearScreen();
        cout << "\t\t\t\033[30;107m||============================================================||\033[0m" << endl;
        cout << "\t\t\t\033[30;107m||                   REGISTERING ACCOUNTS                     ||\033[0m" << endl;
        cout << "\t\t\t\033[30;107m||------------------------------------------------------------||\033[0m" << endl;
        cout << "\t\t\t\033[30;107m||                                                            ||\033[0m" << endl;
        cout << "\t\t\t\033[30;107m||   Username:    "<<setw(44) <<left <<newUser .username <<"||\033[0m" << endl;
        cout << "\t\t\t\033[30;107m||                                                            ||\033[0m" << endl;
        cout << "\t\t\t\033[30;107m||   Password:    ""                                            ||\033[0m" << endl;
