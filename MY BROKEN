        if (outFile.is_open()) {
            outFile << newUser .username << ","
                    << newUser .password << ","
                    << newUser .fullName << ","
                    << newUser .program << ","
                    << newUser .section << ","
                    << newUser .gender << ","
                    << newUser .birthday << ","
